<h2>New post</h1>
<hr>
<p>Find me in app/views/posts/new.html.erb</p>

<%= form_for @post, html: {multipart: true} do |f|%>
    <%= f.label :image%>
    <br>

    <%= f.file_field :image%>
    <br>
    
    <%=f.label :description%>
    <%= f.text_area :description%>
    <br>
    <%=f.submit%>
<%end%>